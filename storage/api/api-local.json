{
    "tiny.video.list": {
        "schema": "http",
        "domain": "localhost",
        "share_domain": "localhost",
        "ip": "127.0.0.1",
        "port": 8000,
        "host": "127.0.0.1",
        "rootPath": "http://localhost:8000",
        "method": "get",
        "path": "/videos",
        "parameters": [
            {
                "name": "user_id",
                "in": "query",
                "type": "integer",
                "default": "100000000000000000"
            },
            {
                "name": "offset",
                "in": "query",
                "type": "integer",
                "default": "0"
            },
            {
                "name": "limit",
                "in": "query",
                "type": "integer",
                "default": "20"
            },
            {
                "name": "updown",
                "in": "query",
                "type": "integer",
                "default": "-1"
            },
            {
                "name": "fields",
                "in": "query",
                "type": "integer"
            },
            {
                "name": "sort",
                "in": "query",
                "type": "integer"
            }
        ],
        "url": "http://localhost:8000/videos"
    },
    "tiny.video.show": {
        "schema": "http",
        "domain": "localhost",
        "share_domain": "localhost",
        "ip": "127.0.0.1",
        "port": 8000,
        "host": "127.0.0.1",
        "rootPath": "http://localhost:8000",
        "method": "get",
        "path": "/videos/:video_id",
        "parameters": [
            {
                "name": "video_id",
                "in": "path",
                "required": true,
                "type": "integer",
                "default": "100000000000000000"
            },
            {
                "name": "user_id",
                "in": "query",
                "type": "integer",
                "default": "100000000000000000"
            },
            {
                "name": "fields",
                "in": "query",
                "type": "integer"
            }
        ],
        "url": "http://localhost:8000/videos/:video_id"
    }
}